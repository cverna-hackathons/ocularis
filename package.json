{
  "name": "ocularis",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon app.js && npm run watch:js",
    "prebuild:js": "rimraf public/js/frontend.js",
    "build:js": "rollup -i ./frontend/app.js > tmp.js && babel --presets es2015 --out-file ./public/js/frontend.js < tmp.js",
    "postbuild:js": "rimraf tmp.js",
    "prewatch:js": "npm run build:js",
    "watch:js": "watch 'npm run build:js' ./frontend/."
  },
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "body-parser": "~1.12.0",
    "compression": "~1.4.1",
    "cookie-parser": "~1.3.3",
    "express": "~4.12.0",
    "express-handlebars": "~2.0.1",
    "glob": "~5.0.3",
    "method-override": "~2.3.0",
    "morgan": "~1.5.0",
    "nodemon": "^1.8.1",
    "pg": "~4.3.0",
    "pg-hstore": "2.3.2",
    "rollup": "^0.25.4",
    "serve-favicon": "~2.2.0",
    "twitter": "^1.2.5",
    "underscore": "^1.8.3",
    "watch": "^0.17.1"
  },
  "devDependencies": {}
}
