{
  "name": "ocularis",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run watch:js & nodemon app.js",
    "prebuild:js": "rimraf public/js/*.js",
    "postbuild:js": "echo 'Built, watching...' & exit 0",
    "build:js": "npm run compile:vr-frontend & npm run compile:ui-frontend",
    "compile:vr-frontend": "rollup -i ./frontend/vr.js > tmp/tmp-vr.js && babel --presets es2015 --out-file ./public/js/vr.js < tmp/tmp-vr.js",
    "compile:ui-frontend": "rollup -i ./frontend/ui.js > tmp/tmp-ui.js && babel --presets es2015 --out-file ./public/js/ui.js < tmp/tmp-ui.js",
    "prewatch:js": "npm run prebuild:js",
    "watch:js": "watch 'npm run build:js' ./frontend/.",
    "build:components": "node scripts/build_components",
    "seed:all": "node scripts/seed_database",
    "register:component": "node scripts/register_component"
  },
  "dependencies": {
    "async": "^1.5.2",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "bcrypt": "^0.8.5",
    "body-parser": "~1.12.0",
    "compression": "~1.4.1",
    "cookie-parser": "~1.3.3",
    "express": "~4.12.0",
    "express-handlebars": "~2.0.1",
    "express-session": "^1.13.0",
    "glob": "~5.0.3",
    "jquery": "^2.2.1",
    "method-override": "~2.3.0",
    "morgan": "~1.5.0",
    "nodemon": "^1.8.1",
    "ocularis-cube": "0.0.4",
    "parallelshell": "^2.0.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-local": "^1.0.0",
    "pg": "~4.3.0",
    "pg-hstore": "2.3.2",
    "q": "^1.4.1",
    "rollup": "^0.25.4",
    "sequelize": "^3.19.3",
    "sequelize-cli": "^2.3.1",
    "serve-favicon": "~2.2.0",
    "three": "^0.74.0",
    "twitter": "^1.2.5",
    "underscore": "^1.8.3",
    "watch": "^0.17.1"
  },
  "devDependencies": {
    "connect-pg-simple": "^3.1.0",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.4.0"
  }
}
